{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "predeploy": ["yarn build"],
    "site": "ftc-teamtrack",
    "public": "dist/spa",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
